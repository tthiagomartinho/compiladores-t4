/*
  Algoritmo Teste 1
*/
  
algoritmo teste1;

variaveis
  valor1, valor2 : real;
  res : inteiro;
  operacao : inteiro;
  l : logico;
fim-variaveis

inicio
  
  operacao = 6;
	avalie (operacao):
        caso 1:
          operacao = operacao + 1;
          pare;
        caso 2:
          operacao = operacao + 2;
          pare;
        caso 3:
          operacao = operacao + 3;
          pare;
        caso 4:
          operacao = operacao + 4;
          pare;
        caso 5:
          operacao = operacao + 5;
          pare;
      fim-avalie

  imprima(operacao);
fim