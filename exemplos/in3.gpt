algoritmo teste3;

variaveis
  string, string2 : literal;
  x, y : real;
fim-variaveis

funcao fatorial (x : inteiro) : inteiro
	aux : inteiro;
inicio
	se x == 1 entao
		retorne 1;
	senao
		aux = x - 1;
		aux =  fatorial(aux);
		retorne ;
	fim-se
fim
fim-funcao
 
inicio
	string = "o fatorial de ";
	string2 = " vale ";
	x = leia();
	y = fatorial(x);
	imprima(string, x, string2, y);
fim